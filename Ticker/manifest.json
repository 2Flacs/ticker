{
	"manifest_version": 2,
	"name": "The Ticker",
	"description": "This extension shows a constant scrolling text of information",
	"version": "1.0",
	
	"browser_action": {
		"default_icon": "check.png",
		"default_popup": "Ticker.html",
		"default_title": "Click here!"
	},
	
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}